{
  "manifest_version": 2,
  "name": "Unique Passwords",
  "version": "1.0.0",
  "description": "Generates unique passwords based on the domain name, user name, and master password.",
  "icons": {
    "16": "icons/icon.svg",
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "64": "icons/icon.svg",
    "96": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "permissions": [
    "menus",
    "activeTab",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "@unique-passwords"
    }
  }
}
